{
  "name": "smart-shopping",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "prettier": "npx prettier --write .",
    "prepare": "husky install",
    "dev:ssr": "ng run smart-shopping:serve-ssr",
